@using MvcFirmaCagri.Models.Entity
@model List<TblCagrilar>
@{
    ViewBag.Title = "PasifCagrilar";
    Layout = "~/Views/Shared/FirmaLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Konu</th>
        <th>Tarih</th>
        <th>Detaylar</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.Konu</td>
            <td>@item.Tarih</td>       
            <td><a href="/Default/CagriDetay/@item.ID/" class="btn btn-info">Detaylar</a></td>
        </tr>
    }
</table>
<a href="/Default/YeniCagri" class="btn btn-primary">Yeni Çağrı Ekle</a>
